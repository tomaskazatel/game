{
    "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
    "rules": {
        "indent": [true, "spaces", 4],
        "import-name": false,
        "variable-name": false,
        "object-shorthand-properties-first": false,
        "no-else-after-return": false,
        "no-boolean-literal-compare": true
    }
}
